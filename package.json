{
  "name": "dongin-secure",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --win portable"
  },
  "devDependencies": {
    "electron": "^33.0.2",
    "electron-builder": "^24.13.3"
  },
  "build": {
    "appId": "com.dongin.secure",
    "productName": "DONGIN SECURE",
    "win": {
      "target": "portable",
      "icon": "icon.ico"
    },
    "portable": {
      "artifactName": "${productName}_v${version}.exe",
      "unpackDirName": "dongin_temp"
    }
  }
}